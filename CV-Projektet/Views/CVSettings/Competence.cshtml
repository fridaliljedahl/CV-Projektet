@model CVDetails
@using Microsoft.AspNetCore.Identity
@inject SignInManager<User> SignInManager
@inject UserManager<User> UserManager

<h2 class="competence">Kompetenser</h2>

@try
{
	<ul>
		@foreach (var item in Model.CompetenceList)
		{
			<li>@item.Name</li>
		}
	</ul>
}
catch (Exception ex) { }
<form id="SettingsCV" method="post" asp-controller="CVSettings" asp-action="AddCompetence">
	<p>Lägg till en ny kompetens:</p>
	<input class="SettingsCV" name="Name" />
	<br />
	<br />
	<input type="submit" value="Spara">
</form>
<p>@Model.ErrorMessage</p>

